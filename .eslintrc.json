{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "operator-linebreak": ["error", "after"],
    "no-unused-vars": "warn",
    "consistent-return": "off"
  }
}
